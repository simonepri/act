{
  "private": true,
  "name": "act",
  "version": "0.1.1",
  "description": "Multi-purpose URI tracker",
  "license": "MIT",
  "scripts": {
    "start": "vercel dev",
    "lint": "xo",
    "test": "xo",
    "update": "npx npm-check -u",
    "release": "npx np"
  },
  "files": [
    "src"
  ],
  "engine": {
    "node": ">=22"
  },
  "dependencies": {
    "@vercel/functions": "^3.1.0",
    "googleapis": "^160.0.0",
    "ua-parser-js": "^2.0.5",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "vercel": "*",
    "xo": "*"
  },
  "xo": {
    "prettier": true,
    "space": true,
    "rules": {
      "object-shorthand": "off",
      "prefer-destructuring": "off"
    }
  }
}
